<mujoco>
    <!-- === Configuration parameter stuff, default values === -->
    <default>
        <default class="position">
            <position ctrllimited="true" forcelimited="false" />
        </default>
        <default class="geom0">
            <geom conaffinity="0" contype="1" friction="1 0.005 0.001" margin="0.001" rgba="0.75 0.6 0.5 1" solimp="0.95 0.95 0.01" solref="0.008 1" />
        </default>
        <default class="link">
            <joint armature="0.1" damping="5" limited="true" stiffness="0" />
        </default>
    </default>
    <!-- === Physical robot definitions === -->
    <worldbody>
        <light diffuse="1.0 1.0 1.0" pos="0 0 10" dir="0 0 -1" />
        <geom type="plane" size="100 100 0.1" rgba="0.46 0.86 1 1" />
        <!--
		-->
    <body name="base" pos="0 0 0.04">
            <geom size="0.05" type="sphere" />
               
                <joint axis="1 0 0" name="base_joint_10" pos="0 0 0" range="-40.5 40.5" type="slide" />
            <body name="base_link" pos="0.05 0 0">
                <geom class="geom0" fromto="0 0 0 0.5 0 0" size="0.035" type="capsule" />
                <joint axis="0 0 1" class="link" name="base_joint" pos="0 0 0" range="-40.5 40.5" type="hinge" />
                <body name="link_0" pos="0.5 0 0">
                    <geom fromto="0 0 0 0.5 0 0" size="0.035" type="capsule" rgba="0.4 0.1 1 1" />
                    <joint axis="0 0 1" class="link" name="joint_0" pos="0 0 0" range="-40.5 40.5" type="hinge" stiffness="10" />
                    <body name="link_1" pos="0.5 0 0">
                        <geom fromto="0 0 0 0.5 0 0" size="0.035" type="capsule" rgba="0.1 0 1 1" />
                        <joint axis="0 0 1" class="link" name="joint_1" pos="0 0 0" range="-40.5 40.5" type="hinge" stiffness="10" />
                        <body name="link_2" pos="0.5 0 0">
                            <geom fromto="0 0 0 0.5 0 0" size="0.035" type="capsule" rgba="0.4 0.1 1 1" />
                            <joint axis="0 0 1" class="link" name="joint_2" pos="0 0 0" range="-40.5 40.5" type="hinge" stiffness="10" />
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>
    <!--
		-->
    <actuator>
        <motor class="position" ctrlrange="-40.5 40.5" joint="base_joint" name="act_base_joint" />
        <position ctrlrange="-40.5 40.5" joint="base_joint_10" name="slid1e" />
        <motor class="position" ctrlrange="-40.5 40.5" joint="joint_0" name="act_joint_0" />
        <motor class="position" ctrlrange="-40.5 40.5" joint="joint_1" name="act_joint_1" />
        <motor class="position" ctrlrange="-40.5 40.5" joint="joint_2" name="act_joint_2" />
    </actuator>
    <option gravity="0 0 -1.8" />
</mujoco>